class Solution {
public:
    map<char, int> extract(string s) {
        map<char, int> mp;
        for (auto it : s) {
            mp[it]++;
        }
        return mp;
    }
    bool canConstruct(string ransomNote, string magazine) {
        map<char, int> mp2 = extract(magazine);

        for (auto it : ransomNote) {
            mp2[it]--;
        }

        for (auto it : mp2) {
            if (it.second < 0) {
                return false;
            }
        }
        return true;
    }
};
