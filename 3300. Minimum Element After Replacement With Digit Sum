class Solution {
public:
    int digitSum(int n) {
        int sum = 0;
        while (n > 0) {
            sum += n % 10;
            n /= 10;
        }
        return sum;
    }

    int minElement(vector<int>& nums) {
        vector<int> v;
        for (int i = 0; i < nums.size(); i++) {
            v.push_back(digitSum(nums[i]));
        }
        sort(v.begin(), v.end());
        return v[0];
    }
};
