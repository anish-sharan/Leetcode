class Solution {
public:
    int getHighest(map<int, int> mp) {
        int highest = 0;
        for (auto it : mp) {
            if (it.second > highest) {
                highest = it.second;
            }
        }
        return highest;
    }
    int maxFrequencyElements(vector<int>& nums) {
        map<int, int> mp;
        int count = 0;
        for (int i = 0; i < nums.size(); i++) {
            mp[nums[i]]++;
        }
        int highest = getHighest(mp);
        for (auto it : mp) {
            if (it.second == highest) {
                count += it.second;
            }
        }
        return count;
    }
};
