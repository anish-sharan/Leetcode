class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {

        set<int> a, res;
        vector<int> ans;

        for (auto it : nums1)
            a.insert(it);

        for (auto it : nums2) {
            if (a.find(it) != a.end())
                res.insert(it);
        }

        for (auto it : res) {
            ans.push_back(it);
        }
        return ans;
    }
};
