class Solution {
public:
    string kthDistinct(vector<string>& arr, int k) {
        map<string, int> mp;
        int count = 0;

        for (auto it : arr) {
            mp[it]++;
        }

        for (int i = 0; i < arr.size(); i++) {
            if (mp[arr[i]] == 1) {
                count++;
            } 
            if (count == k) {
                return arr[i];
            }
        }

       return "";
    }
};
