class Solution {
public:
    vector<int> twoOutOfThree(vector<int>& nums1, vector<int>& nums2,
                              vector<int>& nums3) {
        set<int> s1(nums1.begin(), nums1.end());
        set<int> s2(nums2.begin(), nums2.end());
        set<int> s3(nums3.begin(), nums3.end());
        map<int, int> mp;
        vector<int> res;

        for (auto it : s1) {
            mp[it]++;
        }

        for (auto it : s2) {
            mp[it]++;
        }

        for (auto it : s3) {
            mp[it]++;
        }

        for (auto it : mp) {
            if (it.second >= 2) {
                res.push_back(it.first);
            }
        }
        return res;
    }
};
